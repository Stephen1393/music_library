  "bugs": {
    "url": "https://github.com/Stephen1393/music_library/issues"
  },
  "homepage": "https://github.com/Stephen1393/music_library#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "chai": "^6.2.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "mocha": "^11.7.5",
    "nodeman": "^1.1.2",
    "nodemon": "^3.1.11",
    "superset": "^2.0.1",
    "supertest": "^7.2.2"
  },
  "dependencies": {
    "express": "^5.2.1",
    "pg": "^8.16.3",
    "postgres-migrations": "^5.3.0"
  }
